{
  "authors": [
    "Cristi Constantin <cristi.constantin@live.com>"
  ],
  "bin": {
    "clean-mark": "./bin/clean-mark"
  },
  "bugs": {
    "url": "https://github.com/croqaz/clean-mark/issues"
  },
  "dependencies": {
    "a-extractor": "2.x",
    "breakdance": "3.x",
    "charset": "1.x",
    "cheerio": "^1.0.0-rc.2",
    "chrono-node": "1.x",
    "iconv-lite": "0.4.x",
    "minimist": "1.x",
    "minimize": "2.x",
    "node-fetch": "2.x",
    "sanitize-html": "1.20.x",
    "snapdragon": "0.12.x",
    "voca": "1.x"
  },
  "description": "Convert an article into a clean Markdown",
  "devDependencies": {
    "micro": "9.x",
    "micro-cors": "0.x",
    "mocha": "6.x",
    "prettier": "1.18.2",
    "should": "13.x",
    "standard": "14.3.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "keywords": [
    "cli",
    "clearly",
    "readability",
    "markdown"
  ],
  "license": "MIT",
  "main": "lib/clean.js",
  "name": "clean-mark",
  "overrides": [
    {
      "files": "lib/*.js",
      "options": {
        "printWidth": 120,
        "semi": false,
        "singleQuote": true
      }
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/croqaz/clean-mark.git"
  },
  "scripts": {
    "format": "npx prettier --write lib/*.js && npx standard --fix lib/**.js",
    "lint": "npx standard lib/**.js",
    "start": "node index.js",
    "test": "npx mocha $(find . -name './test/*.test.js')"
  },
  "version": "0.7.0"
}
